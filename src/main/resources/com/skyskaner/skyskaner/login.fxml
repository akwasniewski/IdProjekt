<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.skyskaner.skyskaner.LoginController"
            prefHeight="400.0" prefWidth="600.0">
    <TextField fx:id="usernameField" layoutX="38.0" layoutY="43.0" prefHeight="35.0" promptText="Username" prefWidth="250.0">
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
    </TextField>
    <TextField fx:id="passwordField" layoutX="38.0" layoutY="100.0" prefHeight="35.0" promptText="Password" prefWidth="250.0">
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
    </TextField>
    <Button fx:id="submit" layoutX="38.0" layoutY="140" mnemonicParsing="false" onAction="#Login" prefHeight="26.0" prefWidth="52.0" text="Log In" />
    <Label fx:id="errorLabel" alignment="CENTER"  layoutX="100" layoutY="180" prefHeight=".0" text="" textAlignment="CENTER" textFill="RED"/>
</AnchorPane>
